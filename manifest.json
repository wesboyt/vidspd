{

  "description": "Changes playback speed regardless of UI",
  "manifest_version": 2,
  "name": "vidspd",
  "version": "1.0",
  "homepage_url": "https://github.com/wesboyt/vidspd",
  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": {
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png"
    }
  }

}
